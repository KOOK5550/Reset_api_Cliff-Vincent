{
	"name": "nextchat",
	"version": "4.5.0",
	"description": "A package for interacting with GPT4-based chat services. Gemini, BARD, GPT4, LLaMa, CodeLLaMa, Mixtral and Prodia",
	"main": "main/nextchat.js",
	"module": "main/nextchat.esm.mjs",
	"bin": {
		"nextchat": "cli.js"
	},
	"exports": {
		".": {
			"import": "./main/nextchat.esm.mjs",
			"require": "./main/nextchat.js",
			"default": "./main/nextchat.js"
		},
		"./*": {
			"import": "./main/*.esm.mjs",
			"require": "./main/*.js",
			"default": "./main/*.js"
		}
	},
	"author": "iscordian",
	"license": "MIT",
	"keywords": [
		"ai",
		"chatbot",
		"chatgpt",
		"gpt",
		"bard",
		"llama",
		"bing",
		"api",
		"gpt",
		"discord",
		"chat",
		"chatbot",
		"chatgpt4",
		"chatgpt-4",
		"openai",
		"open-ai",
		"bot",
		"robot",
		"axios",
		"free",
		"stable"
	],
	"dependencies": {
		"axios": "^1.6.3",
    "colors": "^1.4.0",
    "readline": "^1.3.0"
	}
}
